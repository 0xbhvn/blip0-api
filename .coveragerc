[run]
source = src
branch = True
omit = 
    */tests/*
    */__pycache__/*
    */migrations/*
    */alembic/*
    */scripts/*
    */__init__.py

[report]
precision = 2
show_missing = True
skip_covered = False
skip_empty = True
exclude_lines =
    pragma: no cover
    def __repr__
    def __str__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    if typing.TYPE_CHECKING:
    @abstractmethod
    @abc.abstractmethod

[html]
directory = htmlcov

[xml]
output = coverage.xml